<plugin>
   <extension-point id="visualizations" name="Visualizations" schema="schema/visualizations.exsd"/>
   <extension-point id="filters" name="Filters" schema="schema/filters.exsd"/>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.uka.ipd.sdq.edp2.models.ExperimentData.presentation.ExperimentDataModelWizardID"
            name="%_UI_ExperimentDataModelWizard_label"
            class="de.uka.ipd.sdq.edp2.models.ExperimentData.presentation.ExperimentDataModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ExperimentDataModelFile.gif">
         <description>%_UI_ExperimentDataModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="de.uka.ipd.sdq.edp2.models.ExperimentData.presentation.ExperimentDataEditorID"
            name="%_UI_ExperimentDataEditor_label"
            icon="icons/full/obj16/ExperimentDataModelFile.gif"
            extensions="edp2"
            class="de.uka.ipd.sdq.edp2.models.ExperimentData.presentation.ExperimentDataEditor"
            contributorClass="de.uka.ipd.sdq.edp2.models.ExperimentData.presentation.ExperimentDataActionBarContributor">
      </editor>
      <editor
            class="de.uka.ipd.sdq.edp2.models.Repository.presentation.RepositoryEditor"
            contributorClass="de.uka.ipd.sdq.edp2.models.Repository.presentation.RepositoryActionBarContributor"
            extensions="repository"
            icon="icons/full/obj16/RepositoryModelFile.gif"
            id="de.uka.ipd.sdq.edp2.models.Repository.presentation.RepositoryEditorID"
            name="%_UI_RepositoryEditor_label">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.uka.ipd.sdq.edp2.models.Repository.presentation.RepositoryModelWizardID"
            name="%_UI_RepositoryModelWizard_label"
            class="de.uka.ipd.sdq.edp2.models.Repository.presentation.RepositoryModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RepositoryModelFile.gif">
         <description>%_UI_RepositoryModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>


</plugin>
